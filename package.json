  "scripts": {
    "dev": "SOURCE_MAP=true NODE_ENV=local vue-cli-service serve ./src/main.js",
    "build": "rimraf ./dist && NODE_ENV=production vue-cli-service build ./src/main.js",
    "report": "vue-cli-service build ./src/main.js --report ",
    "build:dev": "rimraf ./dist && NODE_ENV=development vue-cli-service build ./src/main.js",
    "build:test": "rimraf ./dist && NODE_ENV=test vue-cli-service build ./src/main.js",
    "test": "vue-cli-service test:unit",
    "unit:watch": "vue-cli-service test:unit --watchAll",
    "inspect": "vue-cli-service inspect > serv.js",
    "format": "prettier --write './src/**/*.{js,vue,json}' && eslint ./src/**/*.{js,vue} --fix",
    "commit": "git-cz"
  },
  "lint-staged": {
    "./src/**/*.{js,vue,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
