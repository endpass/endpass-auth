    "reset.css": "^2.0.2",
    "vue": "^2.5.17",
    "vue-cli-plugin-svg": "^0.1.2",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "web3": "1.0.0-beta.33"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/parser": "^7.4.2",
